<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<!-- Default NuGet source -->
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />

		<!-- Add additional sources if needed (e.g., private feeds) -->
		<!-- Example: <add key="MyPrivateFeed" value="https://my-private-feed-url" /> -->
	</packageSources>

	<packageSourceCredentials>
		<!-- Add credentials for private feeds if required -->
		<!-- Example:
    <MyPrivateFeed>
      <add key="Username" value="your-username" />
      <add key="ClearTextPassword" value="your-password" />
    </MyPrivateFeed>
    -->
	</packageSourceCredentials>

	<!-- Optional: Clear any machine/user-level package sources to ensure consistency -->
	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
	</packageSources>
</configuration>
